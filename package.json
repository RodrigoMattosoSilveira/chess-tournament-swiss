{
  "name": "chess-tournament-swiss",
  "description": "A service to manage a chess tournament using the Swiss pairing method",
  "version": "0.0.2-alpha.1",
  "main": "index.js",
  "repository": "git@github.com:RodrigoMattosoSilveira/chess-tournament-swiss.git",
  "author": "Rodrigo Silveira <r3850365@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "create-t": "node ../node-scripts/create-tournaments index",
    "create-u": "node ../node-scripts/create-users index",
    "get-t-u": "node ../node-scripts/get-users-tournament index 1 5",
    "create-t-u": "run-s create-t create-u get-t-u",
    "start:dev": "NODE_ENV=development NODE_DATA=MONGODB NODE_OPTIONS=--unhandled-rejections=strict webpack",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test_umw": "NODE_ENV=test                     jest --detectOpenHandles -t 'User Middleware Unit Tests'",
    "test_umg": "NODE_ENV=test                    jest --detectOpenHandles -t 'User MongoDB Unit Tests'",
    "test_utils": "NODE_ENV=test                    jest --detectOpenHandles -t 'Util Unit Tests'",
    "test_t": "NODE_ENV=test NODE_DATA=generated jest --detectOpenHandles -t 'Tournament Entity'",
    "test_p": "NODE_ENV=test NODE_DATA=generated jest --detectOpenHandles -t 'Player Entity'",
    "test_g": "NODE_ENV=test NODE_DATA=generated jest --detectOpenHandles -t 'Game Entity'",
    "test_r": "NODE_ENV=test NODE_DATA=generated jest --detectOpenHandles -t 'Round Entity'",
    "set-env": "run-s env-user env-tournament",
    "env-tournament": "REST_API_TOURNAMENT_ID=Zvei69rE0",
    "env-user": "REST_API_USER_ID=xuBs3_tvU"
  },
  "dependencies": {
    "@rmstek/rms-ts-monad": "^1.0.1",
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "mongoose": "^5.12.0",
    "shortid": "^2.2.16",
    "space-monad": "^1.0.0",
    "ts-transformer-keys": "^0.4.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.21",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2"
  }
}
